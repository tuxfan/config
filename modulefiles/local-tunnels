#%Module1.0#####################################################################

#------------------------------------------------------------------------------#
# Local SSH Tunnels
#------------------------------------------------------------------------------#

set-alias u22-pro-up "ssh -fNTML 10022:u22-pro:22 u22-pro-tunnel"
set-alias u22-pro-st "ssh -TO check u22-pro-tunnel"
set-alias u22-pro-dn "ssh -TO exit u22-pro-tunnel"
